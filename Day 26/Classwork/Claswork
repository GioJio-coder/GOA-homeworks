def greet(name):
    print("გამარჯობა,", name)
greet("Giorgi")

def manual_range(start, end, step):
    range1 = range(start, end, step)

    for x in range1:
        if x %2 == 0: print(x)

manual_range(2, 20, 2)
manual_range(20, 120, 5)
manual_range(1, 21, 6)
manual_range(24, 270, 21)
manual_range(20, 200, 10)